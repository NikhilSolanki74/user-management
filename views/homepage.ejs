<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Homepage</title>
    <script src="https://kit.fontawesome.com/7fc8b55b10.js" crossorigin="anonymous"></script>

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
      crossorigin="anonymous"
    />
    <link rel="icon" href="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTMw1mc-2aG3FFNUAelzgUqS9wgjwQETIXZKQ&s" type="image/x-icon">
   
    <style>
      .card-container {
    display: flex;
    flex-wrap: wrap;
   
    gap: 40px;
    
  }

  .card {
    margin-left: 50px;
    width: 18rem;
    margin-bottom: 20px;
  }

    </style>
</head>
<body >
<script>
  history.pushState(null, '', '/userRoute');
</script>
  <%-include('./partials/Notification.js') %>
  <% var homePageData %>
    <%-include('./partials/Navbar.ejs') %>
    <div class="card-container mt-5">

      

      <% if(homePageData){ %>
        <% homePageData.forEach((user) => { %>
        <div class="card">
          
          <img height="200px" width="250px" style="overflow: hidden;" src="<%=user.image%>" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">Name : <%=user.name%></h5>
            <p class="card-text"><h6 style="display: inline;">Address</h6> : <%=user.address%></p>
            <a style="display: flex; align-items: end;" href="/profile/<%=user._id%>"  class="btn btn-primary">More Details</a>
          </div>
        </div>
      <% })}else{ %>
        <h4>No Data Available</h4>
        <% } %>
      <% let data ='dfjd' %>
    </div>


    
      <script
   
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
      crossorigin="anonymous"
    ></script>
    
</body>
</html>